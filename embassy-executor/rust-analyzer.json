{
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": [
    "--no-deps"
  ],
  "rust-analyzer.cargo.features": [
    "arch-riscv32",
    "nightly",
    "executor-thread",
    "integrated-timers"
  ],
  "rust-analyzer.hover.show.structFields": 20,
  "rust-analyzer.checkOnSave": true
}
